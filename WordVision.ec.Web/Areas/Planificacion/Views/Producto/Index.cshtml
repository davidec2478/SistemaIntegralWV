@{ 
    ViewData["Tmp"] = localizer["Indicador Estratégico:"];
    ViewData["Objetivo"] =  ViewBag.Indicador;
    ViewData["Caption"] = localizer["Gestionar Producto aquí"];
 }
@using WordVision.ec.Web.Areas.Planificacion.Models
@model ProductoViewModel
<div class="card">

    <div class="col-sm-12 header-button">

        <button type="button" class="btn btn-primary" id="btnViewModel" onclick="jQueryModalGet('/Planificacion/Producto/OnGetCreateOrEdit?id=0&idGestion=@Model.IdGestion&idIndicadorEstra=@Model.IdIndicadorEstrategico','Factor Crítico de Éxito')">
            <i class="fa fa-plus-square"></i>
            Agregar
        </button>

    </div>
    <div id="viewAll" class="card-body table-responsive">
    </div>
</div>


@section Scripts
{
    <script src="~/js/site.js"></script>
    <script>
    $(document).ready(function () {
